{
  "name": "@celleb/mongoorrhea",
  "version": "0.0.1",
  "description": "Restful query builder for mongodb with mongoose",
  "main": "index.js",
  "scripts": {
    "build": "npx tsc && npm run package",
    "build:watch": "npx tsc --watch",
    "package": "node ./scripts/override-package.js",
    "preversion": "npm run test",
    "version": "npm run build && npm run package && git add .",
    "postversion": "git push && git push --tags",
    "test": "npx  jest --runInBand",
    "test:watch": "npx jest --runInBand --watch"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Celleb/mongoorrhea.git"
  },
  "keywords": [
    "Query",
    "builder",
    "mongodb",
    "mongoose",
    "rest"
  ],
  "author": "Jonas <celleb@mrcelleb.com>",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/Celleb/mongoorrhea/issues"
  },
  "homepage": "https://github.com/Celleb/mongoorrhea#readme",
  "dependencies": {
    "@celleb/js-utils": "^1.2.0",
    "mongoose": "^5.9.19"
  },
  "devDependencies": {
    "@types/jest": "^26.0.0",
    "@types/mongoose": "^5.7.27",
    "@types/node": "^14.0.13",
    "@typescript-eslint/eslint-plugin": "^3.3.0",
    "@typescript-eslint/parser": "^3.3.0",
    "jest": "^26.0.1",
    "mongodb-memory-server": "^6.6.1",
    "ts-jest": "^26.1.0",
    "typescript": "^3.9.5"
  }
}
